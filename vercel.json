{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/pain-tracker/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "routes": [
    {
      "src": "/api/stripe/create-checkout-session",
      "dest": "/api/stripe/create-checkout-session.ts"
    },
    {
      "src": "/api/stripe/webhook",
      "dest": "/api/stripe/webhook.ts"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.2.18"
    }
  }
}
