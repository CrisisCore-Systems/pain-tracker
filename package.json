{
  "name": "pain-tracker",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "A comprehensive tool for tracking and managing chronic pain and injuries",
  "author": "CrisisCore-Systems",
  "license": "MIT",
  "homepage": "https://crisiscore-systems.github.io/pain-tracker",
  "repository": {
    "type": "git",
    "url": "https://github.com/CrisisCore-Systems/pain-tracker"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 50",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "husky install",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist --dotfiles true",
    "prebuild": "node scripts/check-env.mjs",
    "proxy:start": "node scripts/api-proxy.mjs",
    "doctor": "node scripts/doctor.js",
    "scan-secrets": "node scripts/scan-secrets.js",
    "check-merge": "node scripts/check-merge-markers.js",
    "check-security": "npm run scan-secrets && node scripts/check-collapse-vectors.js",
    "security-test": "tsx scripts/automated-security-testing.ts",
    "security-audit": "node scripts/security-audit.js",
    "security-full": "npm run security-test && npm run security-audit",
    "check-tree-reasoning": "node scripts/tree-thought-analyzer.js",
    "pre-commit": "node scripts/pre-commit.js",
    "commitlint": "commitlint",
    "audit-security": "node scripts/security-audit.js",
    "update-deps": "node scripts/update-dependencies.js",
    "deploy:status": "node scripts/deploy.js status",
    "deploy:staging": "node scripts/deploy.js deploy staging",
    "deploy:production": "node scripts/deploy.js deploy production",
    "deploy:rollback": "node scripts/deploy.js rollback",
    "deploy:healthcheck": "node scripts/deploy.js healthcheck",
    "deploy:logs": "node scripts/deploy.js logs",
    "deploy:validate": "node scripts/deploy.js validate",
    "release:patch": "node scripts/deploy.js release patch",
    "release:minor": "node scripts/deploy.js release minor",
    "release:major": "node scripts/deploy.js release major",
    "healthcheck": "node scripts/health-check.js"
  },
  "dependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-alert-dialog": "^1.0.4",
    "@react-pdf/renderer": "^4.3.0",
    "@types/crypto-js": "^4.2.2",
    "chart.js": "^4.4.7",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "glob": "^11.0.3",
    "html2canvas": "^1.4.1",
    "http-proxy-middleware": "^3.0.5",
    "i18next": "^25.5.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "immer": "^10.1.3",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.544.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.7.3",
    "recharts": "^3.2.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.7",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^24.3.1",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.6",
    "postcss": "^8.4.27",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^7.1.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
